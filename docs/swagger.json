{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "This is a sample of a REST API",
        "title": "Library Example API",
        "contact": {
            "name": "Tommy Chu",
            "email": "tommychu2256@gmail.com"
        },
        "license": {},
        "version": "1.0"
    },
    "host": "localhost:8081",
    "basePath": "/",
    "paths": {
        "/books": {
            "get": {
                "description": "find all books in the database and serve it",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "books"
                ],
                "summary": "return all books",
                "responses": {
                    "200": {
                        "description": "Read",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "input",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "JSON unmarshal",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "post": {
                "description": "validate a new book and insert it into the database",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "books"
                ],
                "summary": "create a new book",
                "parameters": [
                    {
                        "description": "Stock Keeping Unit",
                        "name": "sku",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The name of the book",
                        "name": "name",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The author's name",
                        "name": "author",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The book's description",
                        "name": "description",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "default": "0",
                        "description": "The book's value",
                        "name": "price",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Created",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "input",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "JSON unmarshal",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/books/{id}": {
            "get": {
                "description": "find a book by id and serve it",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "books"
                ],
                "summary": "return a book",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Book ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Read",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "input",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "JSON unmarshal",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "put": {
                "description": "find a book and update it with new values",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "books"
                ],
                "summary": "update a book",
                "parameters": [
                    {
                        "description": "Stock Keeping Unit",
                        "name": "sku",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The name of the book",
                        "name": "name",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The author's name",
                        "name": "author",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The book's description",
                        "name": "description",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "The book's value",
                        "name": "price",
                        "in": "body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "type": "integer",
                        "description": "Book ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Updated",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "input",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "JSON unmarshal",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "delete": {
                "description": "find a book and delete it",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "books"
                ],
                "summary": "remove a book",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Book ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Deleted",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "input",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "JSON unmarshal",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}